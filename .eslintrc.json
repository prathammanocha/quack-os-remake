{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "overrides": [
    ],
    "plugins": [
        "react",
        "prettier"
    ],
    "ignorePatterns": [
        "node_modules",
        "dist"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off"
    }
}
